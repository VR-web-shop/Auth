{
    "recurseDepth": 10,
    "sourceType": "module",
    "source": {
        "include": [
            "./src/",
            "./sdk/"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(node_modules/|docs)",
        "recursive": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs",
        "readme": "README.md"
    }
}